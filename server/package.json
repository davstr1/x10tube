{
  "name": "x10tube-server",
  "version": "1.0.0",
  "description": "x10tube - YouTube transcript aggregator",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "css": "tailwindcss -i ./src/styles/input.css -o ./public/styles.css --watch",
    "css:build": "tailwindcss -i ./src/styles/input.css -o ./public/styles.css --minify",
    "migrate:shared-items": "tsx src/scripts/migrate-to-shared-items.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.94.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "marked": "^17.0.1",
    "nanoid": "^5.0.4",
    "nodemailer": "^8.0.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^7.0.9",
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
